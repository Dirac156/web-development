<%- include("header") %>
<div class="box" id="heading">
    <h1><%= listTitle %></h1>
</div>

<div class="box">
    <% for (var item = 0; item < newItems.length; item++) { %>
    <div class="item">
        <input type="checkbox">
        <p><%= newItems[item]%></p>
    </div>
    <% } %>

    <form class="item" action="/" method="post">
        <input type="text" name="new_task" placeholder="New Item" autocomplete="off">
        <button type="submit" name="list" value=<%= listTitle %>>+</button>
    </form>
</div>
<%- include("footer") %>